// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>

int main() {
   char str[100],hi[100];
   int i,j,hichar=0;
   printf("Enter String: ");
   fgets(str,sizeof(str),stdin);
   str[strcspn(str,"\n")]=0;
   int length = strlen(str);
   int var=0;
   for(i=0;i<=length;i++){
        int count = 0;
        if(str[i]==' ' || str[i]=='\0'){
           char arr[100];
           int k=0;
           for(j=var;j<i;j++){
            arr[k++]=str[j];
               count++;
           }
           arr[k]='\0';
           if(count>hichar){
               hichar=count;
               strcpy(hi,arr);
           }var=i+1;
       }
   }
   printf("%s",hi);

    return 0;
}
