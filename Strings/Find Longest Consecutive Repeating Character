#include <stdio.h>
#include <string.h>
#include <stdbool.h>
int main()
{
    char S[10],highchar;
    int i,j,higher=0;
    printf("Enter String ");
    scanf("%s",S);
    int length = strlen(S);
    bool visited[length];
    for(int i = 0; i < length; i++) visited[i] = false;
    
    
    for(int i = 0; i < length; i++){
        int count=1;
        if(visited[i]==true) continue;
        
        for(j=i+1;j<length;j++){
            if(S[i]==S[j]) count++;
        }
        if(count)
         if(count>higher){
             higher=count;
             highchar=S[i];
         }
         
    }
             printf("%c oocured %d times highest ",highchar,higher);

    return 0;
}
