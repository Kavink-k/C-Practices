// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() {
    char str[100],arr[100];
    int i,k=0;
    printf("Enter String: ");
   fgets(str,sizeof(str),stdin);
str[strcspn(str,"\n")]=0;
    int length = strlen(str);
    int  var=length-1;
    for(i=length-1;i>=0;i--){
        if(str[i]==' '){

            for(int j=i+1;j<=var;j++){

                printf("%c",str[j]);
            }var=i;printf(" ");
        }
    }for(i=0;str[i]!=' ';i++){
        printf("%c",str[i]);
    }
    

    return 0;
}
