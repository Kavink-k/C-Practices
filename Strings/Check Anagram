// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() {
    char str1[100],str2[100];
    int i,j;
    printf("Enter String: ");
    scanf("%s %s",str1,str2);
    int length1 = strlen(str1);
    int length2 = strlen(str2);
    int m=0;
    if(length1==length2)
        for(i=0;i<length1;i++){
            for(j=0;j<length1;j++){
                if(str1[i]==str2[j]){
                    m++;
                    break;
            }
        }
    }
    if(m==length1)printf("Yes anagram"); else printf("No");


    return 0;
}
