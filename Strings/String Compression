// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
#include <stdbool.h>

int main() {
    char str[100];
    int i,j;
    printf("Enter String: ");
    scanf("%s",str);
    int length = strlen(str);
    bool visited[length];
     for(int i = 0; i < length; i++) visited[i] = false;
    for(i=0;i<length;i++){
        int count=0;
        if(visited[i]==true) continue;
        for(j=0;j<length;j++){
            if(str[i]==str[j]) {
                visited[j]=true; 
                count++;
            }
        }
        printf("%c%d",str[i],count);
    }
    return 0;
}
