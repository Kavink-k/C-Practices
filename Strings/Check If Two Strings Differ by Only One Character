// Check If Two Strings Differ by Only One Character

#include <stdio.h>
#include <string.h>

int main()
{
    char str1[10], str2[10];
    int count = 0;
    printf("Enter first string:\n");
    scanf("%s", str1);
    printf("Enter second string:\n");
    scanf("%s", str2);

    if (strlen(str1) != strlen(str2))
    {
        printf("Strings are of different lengths.\n");
        return 0;
    }
    for (int i = 0; i < strlen(str1); i++)
    {
        if (str1[i] != str2[i])
        {
count++;
        }
        else
        {
break;        }
    }
    if (count == 1)
    {
        printf("Yes.\n");
    }
    else
    {
        printf("No.\n");
    }
}
