#include <stdio.h>
#include <string.h>
#include <stdbool.h>
int main()
{
    char S[10];
    int i,j,k=0,oddcount=0;
    printf("Enter String ");
    scanf("%s",S);
    int length = strlen(S);
    bool visited[length];
    for(int i = 0; i < length; i++) visited[i] = false;

    
    int count[length];
    for(i=0;i<length;i++){
        if(visited[i]==1){
            continue;
        }
        int freq=1;
        
        for(j=i+1;j<length;j++){
            if(S[i]==S[j]){
                visited[j]=true;
                freq++;
            }
            
           
        }
        if(freq%2!=0) oddcount++;
    }
    
 
    if(oddcount<=1){
        printf("Yes Palindrome");
    }else{
                printf("No Palindrome");

    }
    
    return 0;
}
