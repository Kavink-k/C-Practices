#include <stdio.h>
int main() {
    int arr[100],i,j,n,m;
    scanf("%d",&n);
    for(i=0;i<n;i++)scanf("%d",&arr[i]);
    int count=1;
    for(i=0;i<n;i++){
        for(j=i+1;j<n;j++){
        if(arr[i]>arr[j]){
            count=0;
        }
        }
    }
   if(count) printf("Yes"); else printf("No");

    return 0;
}
