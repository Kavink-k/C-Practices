#include <stdio.h>
int main() {
    int arr[100],i,j,n,arr1[100],k=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)scanf("%d",&arr[i]);
    int temp;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
        if(arr[i]<arr[j]){
            temp=arr[i];            
            arr[i]=arr[j];
            arr[j]=temp;        
        }
        }
    }
    for(i=0;i<n;i++){
        int count=1;
        for(j=i+1;j<n;j++){
            if(arr[i]==arr[j]) count=0; break;
        }
        if(count==1)   arr1[k++]=arr[i];
    }


//   for(i=0;i<k;i++)  printf("%d ",arr1[i]);
  
  printf("%d",arr1[k-2]);

    return 0;
}
