// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() {
    // Write C code here
    char str[100],unique[100];
    int i,length;
    printf("Enter String ");
    scanf("%s",str);
    length=strlen(str);
    int k=0;
    for(i=0;i<length;i++){
        int m=0;
        for(int j=0;j<length;j++){
            if(str[i]==str[j]){
                m++;
            } 
        }
        if(m==1){
            unique[k]=str[i];
            k++;
        }
    }
    if(k) printf("%c",unique[1]); else printf("No");
    return 0;
}
