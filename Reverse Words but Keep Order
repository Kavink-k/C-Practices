#include <stdio.h>
#include <string.h>
int main() {
    char str[100];
    int i,length;
fgets(str,sizeof(str),stdin);
str[strcspn(str,"\n")]=0;
length = strlen(str);
    int variable=0;
    for(i=0;i<=length;i++){
        if(str[i]==' ' || str[i]=='\0'){
            for(int j=i-1;j>=variable;j--){
                    printf("%c",str[j]);
                }            variable=i+1;printf(" ");

            }
        }
 

    return 0;
}
