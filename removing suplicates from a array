#include <stdio.h>
int main()
{
    int Arr[10], i, j, size, count;

    printf("Enter size of the array: ");
    scanf("%d", &size);

    printf("Enter elements of the array: ");
    for (i = 0; i < size; i++)
        scanf("%d", &Arr[i]);

    for (i = 0; i < size; i++)
    {
        for (j = i + 1; j < size; j++)
        {
            if (Arr[i] == Arr[j])
                Arr[j] = 0;
            else
                break;
        }
    }
    for (i = 0; i < size; i++)
    {
        if (Arr[i] == 0){
            continue;
}        else{
            printf("%d",Arr[i]);}
    }

    return 0;
}
